    "storage",
    "*://*.steampowered.com/*",
    "*://steamcommunity.com/*",
    "*://*.isthereanydeal.com/",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "img/*.png",
    "img/*.gif",
    "img/*.jpg",
    "img/*.svg",
    "css/*.css",
    "html/options.html",
    "img/*/*.png",
    "img/profile_styles/*/header.jpg",
    "img/profile_styles/*/showcase.png",
    "img/profile_styles/*/style.css",
    "img/profile_styles/*/preview.png",
    "localization/*.json",
    "js/extra/holidayprofile.js",
    "changelog.json"
